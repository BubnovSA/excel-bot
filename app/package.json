{
  "name": "excel-bot",
  "version": "1.0.0",
  "description": "Bot for work with excel table",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && rm -rf ./tsconfig.tsbuildinfo",
    "build": "yarn clean && tsc --project ./tsconfig.json && tscpaths -p ./tsconfig.json -s ./src -o ./build",
    "start": "node ./build/index.js",
    "start:dev": "nodemon --ext \"ts,json\" --exec \"node --inspect=0.0.0.0 -r ts-node/register\" -r tsconfig-paths/register ./src"
  },
  "author": "BubnovSA",
  "license": "ISC",
  "dependencies": {
    "canvas": "^2.11.2",
    "chart.js": "2.9.4",
    "chartjs-node-canvas": "^4.1.6",
    "exceljs": "^4.4.0",
    "fs": "^0.0.1-security",
    "telegraf": "^4.16.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^20.11.28",
    "nodemon": "^2.0.19",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.5"
  }
}
